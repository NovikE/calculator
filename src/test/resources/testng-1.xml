<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Trigonometric suite">
    <groups>
        <run>
            <exclude name="algebra"></exclude>
        </run>
    </groups>
    <test name="Subtraction test">
        <classes>
            <class name="com.epam.cdp.testNG.SubTest"/>
        </classes>
    </test>
    <test name="Sum test">
        <classes>
            <class name="com.epam.cdp.testNG.SumTest"/>
        </classes>
    </test>
    <test name="Cos test">
        <classes>
            <class name="com.epam.cdp.testNG.CosTest"/>
        </classes>
    </test>
    <test name="Sin test">
        <classes>
            <class name="com.epam.cdp.testNG.SinTest"/>
        </classes>
    </test>
    <test name="Tg test">
        <classes>
            <class name="com.epam.cdp.testNG.TgTest"/>
        </classes>
    </test>
    <test name="Ctg test">
        <classes>
            <class name="com.epam.cdp.testNG.CtgTest"/>
        </classes>
    </test>
</suite>